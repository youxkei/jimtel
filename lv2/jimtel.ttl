@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<https://github.com/youxkei/jimtel>
    a lv2:Plugin , lv2:LimiterPlugin;
    doap:name "Jimtel Limiter";
    doap:license <https://creativecommons.org/publicdomain/zero/1.0/>;
    lv2:requiredFeature lv2:inPlaceBroken;
    lv2:optionalFeature lv2:hardRTCapable;
    lv2:port [
            a lv2:AudioPort , lv2:InputPort;
            lv2:index 0;
            lv2:symbol "in_l";
            lv2:name "In L";
    ] , [
            a lv2:AudioPort , lv2:InputPort;
            lv2:index 1;
            lv2:symbol "in_r";
            lv2:name "In R";
    ] , [
            a lv2:AudioPort , lv2:OutputPort;
            lv2:index 2;
            lv2:symbol "out_l";
            lv2:name "Out L";
    ] , [
            a lv2:AudioPort , lv2:OutputPort;
            lv2:index 3;
            lv2:symbol "out_r";
            lv2:name "Out R";
    ] , [
            a lv2:InputPort , lv2:ControlPort;
            lv2:index 4;
            lv2:symbol "in_gain";
            lv2:name "InGain";
            lv2:default 0.0;
            lv2:minimum -100.0;
            lv2:maximum 100.0;
            units:unit units:db;
    ] , [
            a lv2:InputPort , lv2:ControlPort;
            lv2:index 5;
            lv2:symbol "out_gain";
            lv2:name "OutGain";
            lv2:default 0.0;
            lv2:minimum -100.0;
            lv2:maximum 100.0;
            units:unit units:db;
    ] , [
            a lv2:InputPort , lv2:ControlPort;
            lv2:index 6;
            lv2:symbol "limit";
            lv2:name "Limit";
            lv2:default -15.0;
            lv2:minimum -100.0;
            lv2:maximum 0.0;
            units:unit units:db;
    ] , [
            a lv2:InputPort , lv2:ControlPort;
            lv2:index 7;
            lv2:symbol "release";
            lv2:name "Release";
            lv2:default 2000.0;
            lv2:minimum 10.0;
            lv2:maximum 4000.0;
            units:unit units:ms;
    ] , [
            a lv2:InputPort , lv2:ControlPort;
            lv2:index 8;
            lv2:symbol "use_loudness";
            lv2:name "UseLoudness";
            lv2:default 0.0;
            lv2:minimum 0.0;
            lv2:maximum 1.0;
    ] , [
            a lv2:InputPort , lv2:ControlPort;
            lv2:index 9;
            lv2:symbol "infinite_sustain";
            lv2:name "InfiniteSustain";
            lv2:default 0.0;
            lv2:minimum 0.0;
            lv2:maximum 1.0;
    ] .
